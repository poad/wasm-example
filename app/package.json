    "wasm": "wasm-pack build --target web wasm",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "12.1.2",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.3",
    "typescript": "4.6.3"
  }
}
