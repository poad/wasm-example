{
  "name": "wasm-example",
  "private": true,
  "scripts": {
    "preinstall": "cargo install wasm-pack",
    "dev": "yarn wasm && next dev",
    "build": "yarn wasm && next build",
    "start": "yarn wasm && next start",
    "analyze": "ANALYZE=true next build",
    "export": "yarn wasm && next build && next export",
    "wasm": "wasm-pack build --target web wasm",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^12",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12",
    "@types/node": "*",
    "@types/react": "*",
    "eslint": "*",
    "eslint-config-next": "^12",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "*",
    "next-compose-plugins": "*",
    "prettier": "*",
    "swc-loader": "*",
    "typescript": "*"
  }
}
